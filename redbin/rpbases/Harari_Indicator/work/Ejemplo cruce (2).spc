//EJEMPLO DE RECODE DE LOSINDICADORES
RUNDEF programa
   SELECTION ALL

DEFINE INDIC.VARIAXX
AS RECODE INDIC.INDIC
(201 = 1)
(202 = 2)
(203 = 3)
ELSE 0
TYPE INTEGER
RANGE 1 - 3
VARLABEL "DEMOGRAFIA"
VALUELABELS 1 "TGF" 2 "TMI" 3 "TMM"

TABLE TT AS CROSSTABS OF PAIS.PAIS2  BY  PERIODO.ANIO BY  INDIC.VARIAXX
FOR PERIODO.ANIO = 1 AND INDIC.EDAD = 99 AND INDIC.ZONA= 3  AND INDIC.INDIGENA = 1 AND INDIC.SEXO = 3
WEIGHT  INDIC.VALOR
